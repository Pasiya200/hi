  "contributors": [
    "https://www.youtube.com/c/DRIPSOFC"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/zim-bot/zim-bot..git"
  },
  "bugs": {
    "url": "https://github.com/zim-bot/zimbot-v4/issues"
  },
  "licenses": [
    {
      "type": "GPL-3.0",
      "url": "https://github.com/zim-bot/zimbot-v4/blob/released/LICENSE"
    }
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublish": "npm run test",
    "test:watch": "jest --watchAll --coverage",
    "test:unit": "jest",
    "test:type": "flow check",
    "test": "npm run test:type && npm run test:unit",
    "coverage": "flow coverage index.js --color",
    "nuke": "rm -rf node_modules && rm yarn.lock"
  },
  "devDependencies": {
    "flow-bin": "^0.160.0",
    "jest-cli": "^27.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "\\.spec.js$",
    "collectCoverageFrom": [
      "*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "keywords": [
    "CLI",
    "pretty",
    "logging",
    "log",
    "console",
    "terminal",
    "verbose",
    "ansi"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "license": "GPL-3.0",
  "homepage": "https://github.com/zim-bot/zimbot-v4#readme",
  "dependencies": {
    "drips-memes": "^6.0.0"
  }
}
